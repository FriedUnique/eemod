{
  "multipart": [
    {   "apply": { "model": "eemod:block/wire_center" }},
    {   "when": { "north": "true" },
      "apply": { "model": "eemod:block/wire_side" }},
    {   "when": { "east": "true" },
      "apply": { "model": "eemod:block/wire_side", "y": 90 }},
    {   "when": { "south": "true" },
      "apply": { "model": "eemod:block/wire_side", "y": 180 }},
    {   "when": { "west": "true" },
      "apply": { "model": "eemod:block/wire_side", "y": 270 }}
  ]
}